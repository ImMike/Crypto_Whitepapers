		this.curTabWidth = 0;
		this.tabHeights = [];
		this.tabTops = [];
		this._init(container, options);
	};
	$us.WTabs.prototype._cleanUpLayout = $us.WTabs.prototype.cleanUpLayout;
	$us.WTabs.prototype.cleanUpLayout = function(from){
		this._cleanUpLayout(from);
