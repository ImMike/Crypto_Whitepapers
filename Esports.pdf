<script>
$(document).ready(function(){
 
	$("#register_bday" ).datepicker({
		dateFormat: "yy-mm-dd",
		changeMonth: true,
		changeYear: true,
		yearRange: ""+(new Date().getFullYear() - 100)+":" + (new Date().getFullYear() ) +"",
		monthNames: [ 'January', 'February', 'March', 'April', 'May', 'June', 'July', 'August', 'September', 'October', 'November', 'December' ],
		monthNamesShort: [ 'Jan', 'Feb', 'Mar', 'Apr', 'May', 'Jun', 'Jul', 'Aug', 'Sep', 'Oct', 'Nov', 'Dec' ],
		dayNames: [ 'Sunday', 'Monday', 'Tuesday', 'Wednesday', 'Thursday', 'Friday', 'Saturday' ],
		dayNamesShort: [ 'Sun', 'Mon', 'Tue', 'Wed', 'Thu', 'Fri', 'Sat' ],
		dayNamesMin: [ 'Sun', 'Mon', 'Tue', 'Wed', 'Thu', 'Fri', 'Sat' ],
		maxDate: -4984
	});

    function testError(id){
        var element = document.getElementById(id);
        if(element.value.trim().length < 1) {
            return false;
        }
        else {
            return true;
        }
    }
    function testMinLength(id){
        var element = document.getElementById(id);
        if((element.value.trim().length >= 1) && (element.value.trim().length <= 2)) {
            return false;
        }
        else {
            return true;
        }
    }

    function showError(id, message){
        var element = document.getElementById(id);
        var hasError = document.getElementById(id + "-error");
        element.classList.add("has-error");
        hasError.innerHTML = message;
    }

    function hideError(id) {
        var element = document.getElementById(id);
        var hasError = document.getElementById(id + "-error");
        element.classList.remove("has-error");
        hasError.innerHTML = '';
    }

    function validate() {
        var errors = [];
        
        var usernameRegex = /^[aA-zZ0-9_/-]+$/;
        var emailRegex = /[a-z0-9!#$%&'*+/=?^_`{ | }~-]+(?:\.[a-z0-9!#$%&'*+/=?^_`{ | }~-]+)*@(?:[a-z0-9](?:[a-z0-9-]*[a-z0-9])?\.)+[a-z0-9](?:[a-z0-9-]*[a-z0-9])?/;
        var usernameVal = document.getElementById('register_user').value;
        var registerEmailVal = document.getElementById('register_email').value;

        if(!testError('register_bday')) {
            errors['register_bday'] = 'Birthday should not be empty!';
            showError('register_bday', errors['register_bday']);
        }
        else{
            hideError('register_bday');
            delete errors['register_bday'];
        }

        if(!testError('register_user')) {
            errors['register_user'] = 'Username must not be empty!';
            showError('register_user', errors['register_user']);
        } else if(!usernameRegex.test(usernameVal)) {
            errors['usernameRegex'] = 'Invalid username';
            showError('register_user', errors['usernameRegex']);   
        } else if(!testMinLength('register_user')) {
            errors['register_user'] = 'Minimum 3 characters required';
            showError('register_user', errors['register_user']);
        }
        else {
            hideError('register_user');
            delete errors['register_user'];
        }
        

        if(!testError('register_email')) {
            errors['register_email'] = 'Email must not be empty!';
            showError('register_email', errors['register_email']);
        } else if(!emailRegex.test(registerEmailVal)) {
            errors['register_email'] = 'Invalid email';
            showError('register_email', errors['register_email']);
        }
        else{
            hideError('register_email');
            delete errors['register_email'];
        }

        if(!testError('register_confemail')) {
            errors['register_confemail'] = 'Confirmation email must not be empty!';
            showError('register_confemail', errors['register_confemail']);
        }
        else{
            hideError('register_confemail');
            delete errors['register_confemail'];
        }

        if(document.getElementById('register_email').value != document.getElementById('register_confemail').value) {
            errors['register_confemail'] = 'Same email required!';
            showError('register_confemail', errors['register_confemail']);
            showError('register_email', '');
        }

        if(!testError('register_pwd')) {
            errors['register_pwd'] = 'Password required';
            showError('register_pwd', errors['register_pwd']);
        } else if(!testMinLength('register_pwd')) {
            errors['register_pwd'] = 'Minimum 3 characters required';
            showError('register_pwd', errors['register_pwd']);
        }
        else{
            hideError('register_pwd');
            delete errors['register_pwd'];
        }

        if(!testError('register_confpwd')) {
            errors['register_confpwd'] = 'Password required';
            showError('register_confpwd', errors['register_confpwd']);
        }
        else{
            hideError('register_confpwd');
            delete errors['register_confpwd'];
        }

        if(document.getElementById('register_pwd').value != document.getElementById('register_confpwd').value) {
            errors['register_confemail'] = 'Same password required!';
            showError('register_confpwd', errors['register_confemail']);
            showError('register_pwd', '');
        }

        if(!testError('register_fname')) {
            errors['register_fname'] = 'First name required';
            showError('register_fname', errors['register_fname']);
        }
        else{
            hideError('register_fname');
            delete errors['register_fname'];
        }

        if(!testError('register_lname')) {
            errors['register_lname'] = 'Last name required';
            showError('register_lname', errors['register_lname']);
        }
        else{
            hideError('register_lname');
            delete errors['register_lname'];
        }

        if(!document.getElementById('accept-toup').checked) {
            errors['tc-error'] = 'You must agree with the Terms and Conditions';
            document.getElementById('tc-error').innerHTML = errors['tc-error'];
            return false;
        }
        else {
            document.getElementById('tc-error').innerHTML = '';
            delete errors['tc-error'];
            return true;
        }

