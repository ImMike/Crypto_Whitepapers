<script>
    $(document).ready(function() {

        $('#login_user, #login_pwd').keypress(function (e) {
            if (e.keyCode == 13) {
                $('#btn-submit-login').click();
            }
        });

        $('#btn-submit-login').click(function () {

            var formData = $('#form-login').serialize();

            $.post('/login.html', formData).done(function (data) {

                if (data.status == 'success') {

                    window.location.href = "/profile.html";
                }
                else {
                    
                    if(data.errors != undefined){
                        var html = '';
                        $('#login-error').html('');
                        $.each(data.errors, function(i,v){
                            html += '<div class="form-error">'+v+'</div>';
                        });
                        
                        $('#login-error').html(html);
                        
                    }else{
                        $('#login-error').html('');
                        $('#login-error2').text(data.message);                
                    }
                      
                }
            });
        });
    });
</script>
