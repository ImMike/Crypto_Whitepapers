        function verifyCookie() {
            if (document.cookie.replace(/(?:(?:^|.*;\s*)cookie\s*\=\s*([^;]*).*$)|^.*$/, "$1") !== "true") {
                document.cookie = "cookie=true; expires=Fri, 31 Dec 9999 23:59:59 GMT";
