<script>
    document.addEventListener("DOMContentLoaded", function(event) {
        function sendEmail() {
            if($("#email").val().length <= 0) {
                alert("Please enter email");
            } else {
                var emailexp = new RegExp(/\b[A-Z0-9._%+-]+@[A-Z0-9.-]+\.[A-Z]{2,4}\b/i);
                if ($("#email").val().match(emailexp)) {
                    if ($("#subject").val().length <= 0) {
                        alert("Please enter subject");
                    } else {
                        if ($("#message").val().length <= 0) {
                            alert("Please enter message");
                        } else {
                            $("#cover").height($("body").height());
                            $("#cover").fadeIn(100);
                            $.ajax({
                                url: "/api/sendemail",
                                data: {
                                    email: $("#email").val(),
                                    subject: $("#subject").val(),
                                    message: $("#message").val()
                                },
                                cache: false,
                                method: "post",
                                success: function (html) {
                                    if (html == "OK") {
                                        alert("Email was send to info@fidentiax.com successful");
                                        $("#email").val("");
                                        $("#subject").val("");
                                        $("#message").val("");
                                    } else {
                                        alert("Error occur when send email to info@fidentiax.com");
                                    }
                                    $("#cover").fadeOut(100);
                                    $("body").css("overflow", "");
                                },
                                error: function (err) {
                                    console.log(err);
                                    $("#cover").fadeOut(100);
                                    $("#cover").height(0);
                                }
                            });
                        }
                    }
                } else {
                    alert("Wrong email format");
                }
            }
        }
        $(".sendbtn").click(function() {
            sendEmail();
        });
        $(".sendtext").click(function() {
            sendEmail();
        });
        function switchImage(obj, newImage) {
            obj.src = newImage;
        }

        $("[id^=mnlink_]").each(function() {
            var links = this.value;
            var id = this.id.replace("mnlink_", "");
            var linkArr = links.split("#####");
            for (var i = 0; i < linkArr.length;) {
                $("#mnul_" + id).append("<li><a target='_blank' href='" + linkArr[i + 1] + "'>" + linkArr[i] + "</a></li>");
                i = i + 2;
            }
        });

        $("[id^=mn_]").click(function() {
            $("[id^=mnclose_]").click();
            $("#" + this.id + "").next().show();
        });

        $("[id^=mnclose_]").click(function() {
            $("#" + this.id + "").parent().parent().hide();
        });
    });
</script>        </div>
