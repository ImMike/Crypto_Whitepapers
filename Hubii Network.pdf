var tPostSale = moment.tz('2017-09-09 07:02:10', 'Europe/Zurich').toDate()

var countdownEvent = {
    update: 'update.countdown',
    finish: 'finish.countdown'
}

function updateCountdown(event) {
    $(this).html(event.strftime(''
        + '<div class="row font-white text-center">'
        + '  <div class="col-xs-offset-2 col-xs-4 col-sm-offset-2 col-sm-2">'
        + '    <div class="count-number">%D</div>'
        + '    <div class="count-descr">Day%!D</div>'
        + '  </div>'
        + '  <div class="col-xs-4 col-sm-2">'
        + '    <div class="count-number">%H</div>'
        + '    <div class="count-descr">Hour%!H</div>'
        + '  </div>'
        + '  <div class="col-xs-offset-2 col-xs-4 col-sm-offset-0 col-sm-2">'
        + '    <div class="count-number">%M</div>'
        + '    <div class="count-descr">Minute%!M</div>'
        + '  </div>'
        + '  <div class="col-xs-4 col-sm-2">'
        + '    <div class="count-number">%S</div>'
        + '    <div class="count-descr">Second%!S</div>'
        + '  </div>'
        + '</div>'
    ));
}

function finishCountdownAtSale() {
    $('#countdown')
        .countdown(tPostSale)
        .one(countdownEvent.finish, finishCountdownAtPostSale);
}

function finishCountdownAtPostSale() {
    $(this).html('');
}

$(document).ready(function () {

    $('#countdown')
        .countdown(tSale)
        .on(countdownEvent.update, updateCountdown)
        .one(countdownEvent.finish, finishCountdownAtSale);

    $('.next').click(function () {
        var nextId = $(this).parents('.tab-pane').next().attr("id");
        $('[href="#' + nextId + '"]').tab('show');
        return false;
    });

    $('.prev').click(function () {
        var prevId = $(this).parents('.tab-pane').prev().attr("id");
        $('[href="#' + prevId + '"]').tab('show');
        return false;
    });

    $('a[data-toggle="tab"]').on('shown.bs.tab', function (e) {
        //update progress
        var step = $(e.target).data('step');
        var percent = (parseInt(step) / 3) * 100;

        $('.progress-bar').css({width: percent + '%'});

        var text = ''
        switch (step) {
            case 1:
                text = 'T&C';
                break;
            case 2:
                text = 'Buy Hubiits';
                break;
            case 3:
                text = 'What is next?';
                break;
        }
        $('.progress-bar').text(text);

        //e.relatedTarget // previous tab
    });

    $('.first').click(function () {
        $('#sale-wizard a:first').tab('show');
    });

    $('input[type="checkbox"]').click(function () {
        var accepted = $('#sale-accepted').is(':checked');
        var notUS = $('#sale-not-us-or-sg').is(':checked');
        var notSG = $('#sale-origin').is(':checked');
        if (accepted && notUS && notSG) {
            $('#sale-agree').removeClass('disabled');
        } else {
            $('#sale-agree').addClass('disabled');
        }
    })

    new Clipboard('.btn');
});</script></body></html>