window["obvInit"]({"value":{"id":"ffd324bb084","versionId":"e820bc825701","creatorId":"1dfb4b631416","creator":{"userId":"1dfb4b631416","name":"Robin Linus","username":"RobinLinus","createdAt":1460964855026,"lastPostCreatedAt":1513051698494,"imageId":"1*jAusAZ6CEMUDfJozfy8I0w.png","backgroundImageId":"","bio":"Creator of https://nimiq.com","twitterScreenName":"robin_linus","socialStats":{"userId":"1dfb4b631416","usersFollowedCount":18,"usersFollowedByCount":501,"type":"SocialStats"},"social":{"userId":"lo_qKrXYtYIAZhg","targetUserId":"1dfb4b631416","type":"Social"},"facebookAccountId":"","allowNotes":1,"isNsfw":false,"type":"User"},"homeCollection":{"id":"de0ac9fd7e51","name":"Nimiq","slug":"nimiq-network","tags":["BLOCKCHAIN","CRYPTOGRAPHY","TOKEN SALE","ALTCOINS"],"creatorId":"b1e46d500336","description":"Browser-based Blockchain & Ecosystem — https://nimiq.com","shortDescription":"Browser-based Blockchain & Ecosystem — https://nimiq.com","image":{"imageId":"1*bMVnBoaINFxUbfvEtQfa9A.png","filter":"","backgroundSize":"","originalWidth":512,"originalHeight":512,"strategy":"resample","height":0,"width":0},"metadata":{"followerCount":1624,"activeAt":1513217291253},"virtuals":{"permissions":{"canPublish":false,"canPublishAll":false,"canRepublish":false,"canRemove":false,"canManageAll":false,"canSubmit":false,"canEditPosts":false,"canAddWriters":false,"canViewStats":false,"canSendNewsletter":false,"canViewLockedPosts":false,"canViewCloaked":false,"canEditOwnPosts":false,"canBeAssignedAuthor":false,"canEnrollInHightower":false,"canLockPostsForMediumMembers":false},"isSubscribed":false,"isNewsletterSubscribed":false,"memberOfMembershipPlanId":"","isEnrolledInHightower":false,"isEligibleForHightower":false},"logo":{"imageId":"1*mY7tcn3n1jSiwuQTKU00Gg.png","filter":"","backgroundSize":"","originalWidth":256,"originalHeight":256,"strategy":"resample","height":0,"width":0},"twitterUsername":"nimiq","publicEmail":"press@nimiq.com","sections":[{"type":2,"collectionHeaderMetadata":{"title":"Nimiq Network","description":"Browser-based Blockchain & Ecosystem — https://nimiq.com","backgroundImage":{"id":"1*h-YzYk-NtcPufnHRDWHaRg.png","originalWidth":1500,"originalHeight":750},"logoImage":{"id":"1*yG3Iis1GkSL90ieV0OnJow@2x.png","originalWidth":480,"originalHeight":480,"alt":"Nimiq Network"},"alignment":1,"layout":6}},{"type":1,"postListMetadata":{"source":1,"layout":6,"number":20,"postIds":[]}}],"favicon":{"imageId":"","filter":"","backgroundSize":"","originalWidth":0,"originalHeight":0,"strategy":"resample","height":0,"width":0},"colorPalette":{"defaultBackgroundSpectrum":{"colorPoints":[{"color":"#FF02B875","point":0},{"color":"#FF00AB6B","point":0.1},{"color":"#FF1C9963","point":0.2},{"color":"#FF092E20","point":1}],"backgroundColor":"#FFFFFFFF"},"highlightSpectrum":{"colorPoints":[{"color":"#FFFFFFFF","point":0},{"color":"#FFE9FDF0","point":0.1},{"color":"#FFE2FAEE","point":0.2},{"color":"#FFADFFCF","point":0.6},{"color":"#FF7DFFB3","point":1}],"backgroundColor":"#FFFFFFFF"}},"navItems":[{"type":6,"title":"Welcome","url":"https://medium.com/nimiq-network/about"},{"type":3,"title":"Nimiq Betanet","url":"https://www.nimiq.com/betanet"}],"colorBehavior":1,"instantArticlesState":0,"acceleratedMobilePagesState":0,"ampLogo":{"imageId":"","filter":"","backgroundSize":"","originalWidth":0,"originalHeight":0,"strategy":"resample","height":0,"width":0},"header":{"title":"Nimiq Network","description":"Browser-based Blockchain & Ecosystem — https://nimiq.com","backgroundImage":{"id":"1*h-YzYk-NtcPufnHRDWHaRg.png","originalWidth":1500,"originalHeight":750},"logoImage":{"id":"1*yG3Iis1GkSL90ieV0OnJow@2x.png","originalWidth":480,"originalHeight":480,"alt":"Nimiq Network"},"alignment":1,"layout":6},"type":"Collection"},"homeCollectionId":"de0ac9fd7e51","title":"Nimiq: A Frictionless Payment Protocol Native to the Web","detectedLanguage":"en","latestVersion":"e820bc825701","latestPublishedVersion":"e820bc825701","hasUnpublishedEdits":false,"latestRev":689,"createdAt":1497397902857,"updatedAt":1517339916486,"acceptedAt":0,"firstPublishedAt":1497398606524,"latestPublishedAt":1514531117374,"vote":false,"experimentalCss":"","displayAuthor":"","content":{"bodyModel":{"paragraphs":[{"name":"a1e9","type":3,"text":"Nimiq: A Frictionless Payment Protocol Native to the Web","markups":[]},{"name":"45d9","type":1,"text":"中文","markups":[{"type":3,"start":0,"end":2,"href":"https://medium.com/nimiq-translations/nimiq-%E4%B8%80%E4%B8%AAweb%E5%8E%9F%E7%94%9F%E7%9A%84%E6%97%A0%E6%91%A9%E6%93%A6%E7%9A%84%E6%94%AF%E4%BB%98%E5%8D%8F%E8%AE%AE-6a8250b21146","title":"","rel":"","anchorType":0},{"type":1,"start":0,"end":2}]},{"name":"2a43","type":4,"text":"","markups":[],"layout":1,"metadata":{"id":"1*7DSgRazsxGLPKNq0J0wmLw.png","originalWidth":1831,"originalHeight":651}},{"name":"582e","type":3,"text":"Introduction","markups":[]},{"name":"d2aa","type":1,"text":"On June 6th we published the Nimiq Beta Testnet to get early community feedback and gather real-world data. Some people were missing the nerdy talk. So here it is:","markups":[{"type":3,"start":29,"end":47,"href":"https://nimiq.com/betanet/","title":"","rel":"","anchorType":0}]},{"name":"e985","type":1,"text":"Nimiq is a frictionless peer-to-peer payment protocol for the World Wide Web. It is a third-generation Blockchain protocol combining elements of Bitcoin and Ethereum, streamlined for the web platform. And without a doubt, it is open source and fully decentralized.","markups":[{"type":3,"start":228,"end":239,"href":"https://github.com/nimiq-network/core","title":"","rel":"noopener","anchorType":0},{"type":2,"start":0,"end":264}]},{"name":"af2f","type":1,"text":"Browsers are first-class citizens in the Nimiq distributed network. They are able to establish consensus with the network, and enable true peer-to-peer payments from within, all without a trusted third party. In comparison to conventional cryptocurrencies, this browser-first approach lowers barriers of entry by orders of magnitudes for developers, customers and merchants.","markups":[{"type":2,"start":0,"end":374}]},{"name":"d20b","type":3,"text":"Overview","markups":[]},{"name":"0af3","type":1,"text":"The main challenge of a web-based Blockchain is to translate the core Blockchain components to the web platform:","markups":[]},{"name":"0e82","type":9,"text":"Network for establishing P2P connections.","markups":[{"type":1,"start":0,"end":7}]},{"name":"7f2e","type":9,"text":"Storage for persistent keys and Blockchain data.","markups":[{"type":1,"start":0,"end":7}]},{"name":"0b42","type":9,"text":"Crypto for hashing, signing and verifying.","markups":[{"type":1,"start":0,"end":6}]},{"name":"94de","type":1,"text":"In addition, the protocol must be streamlined for the constraints of the web:","markups":[]},{"name":"fc64","type":9,"text":"Compression of Blockchain data to sync within seconds instead of hours.","markups":[{"type":1,"start":0,"end":11}]},{"name":"db73","type":9,"text":"Instant and scalable transactions so over-the-counter payments are practical.","markups":[{"type":1,"start":0,"end":33}]},{"name":"ba20","type":9,"text":"Simplicity means we do only one thing and we do it better than anyone else: payments.","markups":[{"type":1,"start":0,"end":11}]},{"name":"4991","type":9,"text":"Blockchain Parameters streamlined for our browser-first approach.","markups":[{"type":1,"start":0,"end":22}]},{"name":"c34f","type":9,"text":"Cross-Chain Compatibility with other Blockchains such as Ethereum for advanced smart contract features.","markups":[{"type":1,"start":0,"end":25}]},{"name":"49c1","type":3,"text":"Translating the Blockchain Primitives to the Web Platform","markups":[]},{"name":"2be5","type":13,"text":"Network","markups":[]},{"name":"9a1e","type":1,"text":"Nimiq’s peer-to-peer network uses WebRTC and WebSocket connections.","markups":[{"type":3,"start":34,"end":40,"href":"https://developer.mozilla.org/en-US/docs/Web/API/WebRTC_API","title":"","rel":"","anchorType":0},{"type":3,"start":45,"end":54,"href":"https://developer.mozilla.org/en-US/docs/Web/API/WebSocket","title":"","rel":"","anchorType":0}]},{"name":"ef69","type":1,"text":"There are two types of nodes in the Nimiq network: Backbone Nodes and Browser Nodes. Both types use the same isomorphic JavaScript code base.","markups":[{"type":2,"start":51,"end":66},{"type":2,"start":70,"end":85}]},{"name":"8bff","type":1,"text":"Backbone Nodes are based on NodeJS and run on servers. They communicate with each other via WebSockets, and they act as entry point and signaling server for Browser Nodes to establish browser-to-browser WebRTC connections.","markups":[]},{"name":"a1e0","type":1,"text":"Browser Nodes are built upon browser engines and therefore they are completely installation-free. To connect to the network, they establish a WebSocket connection to at least one Backbone Node. Once they have established their first connection, they start to establish browser-to-browser connections using the Backbone Node as signaling server. Browser Nodes can also act as signaling server for further browser-to-browser connections.","markups":[]},{"name":"7466","type":1,"text":"In the long run, Browser Nodes will mainly be light-clients, and they won’t necessarily participate as miners. Their main purpose is to establish consensus quickly to prove their accounts’ balances and send transactions into the network. Serious Miners might prefer to run mainly Backbone Nodes for performance and convenience reasons, even though we want to keep the benefit of running a backbone node low in order to balance incentives in the direction of our browser-first approach. Moreover, even Browsers running light-clients will contribute resources to the network: They share the (compressed) Blockchain data with other browsers to reduce the network load on the backbone nodes.","markups":[]},{"name":"ecb4","type":1,"text":"There are some drawbacks of this approach:","markups":[]},{"name":"f7a4","type":10,"text":"Depending on the user’s NAT configuration, direct peer-to-peer connections may not be able to be established. They would need a TURN server to connect to other browsers. In this case, it makes more sense to connect only to Backbone Nodes via WebSockets, because the network load on TURN servers would be unnecessary high.","markups":[{"type":3,"start":128,"end":139,"href":"https://en.wikipedia.org/wiki/Traversal_Using_Relays_around_NAT","title":"","rel":"noopener","anchorType":0},{"type":2,"start":128,"end":140}]},{"name":"26a7","type":10,"text":"Powerful browser APIs are restricted to secure origins. So for browsers to connect, Backbone Nodes need to provide an encrypted connection via SSL. This requires a domain and an SSL certificate. For easy and cheap access to domains, we will provide dynamic DNS and Letsencrypt packaged in an installer.","markups":[{"type":3,"start":26,"end":54,"href":"https://www.chromium.org/Home/chromium-security/prefer-secure-origins-for-powerful-new-features","title":"","rel":"noopener","anchorType":0},{"type":3,"start":265,"end":276,"href":"https://letsencrypt.org/","title":"","rel":"noopener","anchorType":0}]},{"name":"6a26","type":10,"text":"Depending on the user’s firewall configuration, connections to non-standard ports may not be able to be established. In this case, at least some nodes need to run on the standard 443 port. To avoid running the node as root, it is a good idea to use NGINX as reverse proxy.","markups":[]},{"name":"4273","type":13,"text":"Storage","markups":[]},{"name":"3a28","type":1,"text":"Browser Nodes use the IndexedDB API to store Blockchain data and keys on the user’s hard drive. Since the browser can’t store gigabytes of data, we compress the Blockchain with the Mini-Blockchain scheme (see Compression). In the beta testnet, the private key is stored unencrypted and gets deleted if the user clears his browser history.","markups":[{"type":3,"start":22,"end":35,"href":"https://developer.mozilla.org/en/docs/Web/API/IndexedDB_API","title":"","rel":"noopener","anchorType":0},{"type":2,"start":209,"end":220}]},{"name":"cfb0","type":1,"text":"In the mainnet we will have much higher security standards:","markups":[]},{"name":"e9f2","type":1,"text":"With Chrome’s Storage Persistence API the data survives even if you clear your browser data. The private key will always be stored encrypted. Furthermore, we will provide users with a simple app to backup their key in a printed paper wallet. We are also planning on supporting cold-wallets via Web USB or Web Bluetooth.","markups":[{"type":3,"start":14,"end":37,"href":"https://developers.google.com/web/updates/2016/06/persistent-storage","title":"","rel":"noopener","anchorType":0},{"type":3,"start":294,"end":301,"href":"https://developers.google.com/web/updates/2016/03/access-usb-devices-on-the-web","title":"","rel":"noopener","anchorType":0},{"type":3,"start":305,"end":318,"href":"https://developers.google.com/web/updates/2015/07/interact-with-ble-devices-on-the-web","title":"","rel":"noopener","anchorType":0}]},{"name":"9977","type":3,"text":"Crypto","markups":[]},{"name":"f63a","type":1,"text":"Since performance is security relevant, we need performant crypto primitives. A JavaScript implementation would not be sufficient.","markups":[]},{"name":"1a8c","type":13,"text":"Betanet Crypto","markups":[{"type":1,"start":0,"end":14}]},{"name":"fc7c","type":1,"text":"The crypto in the preliminary beta testnet is based on the WebCrypto APIs to reach near native performance. The WebCrypto API is not very rich and leads to too many sub-optimal design decisions. Therefore we won’t use it for the mainnet.","markups":[{"type":3,"start":59,"end":73,"href":"https://developer.mozilla.org/en-US/docs/Web/API/Web_Crypto_API","title":"","rel":"","anchorType":0}]},{"name":"fe92","type":1,"text":"Proof-of-Work Algorithm: We use SHA-256, because it is the only hash function supported by WebCrypto, and is sufficient for our first public testnet.","markups":[{"type":1,"start":0,"end":23}]},{"name":"a18e","type":1,"text":"Digital Signing Algorithm: We use the NIST curve P-256, because it is the only curve supported by WebCrypto.","markups":[{"type":1,"start":0,"end":26}]},{"name":"00df","type":13,"text":"Mainnet Crypto (preliminary)","markups":[]},{"name":"c8d9","type":1,"text":"The cryptographic primitives in the mainnet will be based on WebAssembly for near native performance and full flexibility in our choice of crypto algorithms.","markups":[{"type":3,"start":61,"end":72,"href":"https://developer.mozilla.org/en-US/docs/WebAssembly","title":"","rel":"","anchorType":0}]},{"name":"3367","type":1,"text":"Proof-of-Work Algorithm: We will use a more sophisticated PoW in the mainnet because SHA-256 will lead to centralized mining. The mainnet PoW should be memory-hard & low energy for truly decentralized mining with regular hardware. There is no final decision on any specific algorithm yet. We are investigating multiple candidates such as Argon2 (too slow though) or ETHash, and we are very open to suggestions by the community.","markups":[{"type":3,"start":338,"end":344,"href":"https://password-hashing.net/submissions/specs/Argon-v3.pdf","title":"","rel":"noopener","anchorType":0},{"type":1,"start":0,"end":24}]},{"name":"74b2","type":1,"text":"Proof-of-Stake Algorithm: We would strongly prefer to switch to a Proof-of-Stake Algorithm in the long run to get to a more energy-efficient Blockchain system. We are following the research of other projects such as Ethereum, and we are investigating multiple candidates such as Ouroboros. Just as well, we are very open to suggestions by the community.","markups":[{"type":3,"start":216,"end":224,"href":"https://github.com/ethereum/wiki/wiki/Proof-of-Stake-FAQ","title":"","rel":"noopener","anchorType":0},{"type":3,"start":279,"end":288,"href":"https://eprint.iacr.org/2016/889.pdf","title":"","rel":"noopener","anchorType":0},{"type":1,"start":0,"end":26}]},{"name":"cd45","type":1,"text":"Digital Signing Algorithm: We use Ed25519 because it uses “nothing up my sleeves parameters” chosen for performance and it is becoming an industry standard.","markups":[{"type":1,"start":0,"end":27}]},{"name":"6df7","type":3,"text":"More Fundamental Browser APIs","markups":[]},{"name":"0c9d","type":1,"text":"ES6 Classes for a clear and simple object oriented design.","markups":[{"type":3,"start":0,"end":11,"href":"https://developer.mozilla.org/en/docs/Web/JavaScript/Reference/Classes","title":"","rel":"","anchorType":0}]},{"name":"06c3","type":1,"text":"Promises and the beloved async/await to escape from callback hell.","markups":[{"type":3,"start":25,"end":36,"href":"https://developer.mozilla.org/en-US/docs/Web/JavaScript/Reference/Statements/async_function","title":"","rel":"","anchorType":0}]},{"name":"5194","type":1,"text":"ArrayBuffer and views to serialize Blockchain data to byte level for crypto operations, network and storage.","markups":[{"type":3,"start":0,"end":11,"href":"https://developer.mozilla.org/en/docs/Web/JavaScript/Reference/Global_Objects/ArrayBuffer","title":"","rel":"noopener","anchorType":0}]},{"name":"64eb","type":3,"text":"Streamlining the protocol for the constraints of the Web Platform","markups":[]},{"name":"24ad","type":13,"text":"Compression","markups":[]},{"name":"6622","type":1,"text":"It is nonsensical to have web users download gigabytes of Blockchain data to establish consensus. Especially in the case of a weak network connection, which would make this nearly impossible. So we need to compress the data such that light-clients can synchronize within seconds.","markups":[]},{"name":"7184","type":1,"text":"That’s where the Mini-Blockchain Scheme comes into play: It introduces an Accounts Tree (a Merkle Patricia Tree) which has multiple advantages in comparison to the design of Bitcoin:","markups":[{"type":3,"start":17,"end":39,"href":"http://cryptonite.info/wiki/index.php?title=Main_Page","title":"","rel":"","anchorType":0},{"type":3,"start":91,"end":111,"href":"https://github.com/ethereum/wiki/wiki/Patricia-Tree","title":"","rel":"","anchorType":0},{"type":2,"start":17,"end":39},{"type":2,"start":74,"end":112}]},{"name":"cfde","type":9,"text":"It simplifies transactions. The end user doesn’t need to deal with the concept of unspent outputs. Only an account and a balance is contained.","markups":[]},{"name":"b084","type":9,"text":"The user can download only the headers of the Blockchain plus simple cryptographic proofs for account balances, which breaks down to downloading only a couple of hundred kilobytes instead of gigabytes without losing trust or security.","markups":[]},{"name":"0e5e","type":9,"text":"Old blocks can be discarded because the complete state is stored in the Accounts Tree.","markups":[]},{"name":"5d57","type":9,"text":"Nano clients can use signed checkpoints to keep the headers chain constant size. Additionally, the full Blockchain up to each checkpoint will be provided for download to make public verification of these checkpoints easy. We will proceed to implement the Mini Blockchain Scheme as per development plan.","markups":[]},{"name":"42b1","type":9,"text":"It is easy to listen for balance changes.","markups":[]},{"name":"b51f","type":13,"text":"Instant transactions","markups":[]},{"name":"29d8","type":1,"text":"Based on “The Bitcoin Lightning Network: Scalable Off-Chain Instant Payments” Nimiq implements Hashed Timelock Contracts to enable payment channels and within those scalable instant off-chain transactions. This solves the scalability issues beyond micro-payments. Moreover, they enable atomic swaps for cross-chain interoperability. There are also approaches to enable onion routing on top the Lightning Network to provide anonymity in a way similar to the Tor Browser.","markups":[{"type":3,"start":10,"end":76,"href":"https://lightning.network/lightning-network-paper.pdf","title":"","rel":"noopener","anchorType":0},{"type":2,"start":9,"end":77},{"type":2,"start":95,"end":121},{"type":2,"start":131,"end":148},{"type":2,"start":286,"end":298},{"type":2,"start":369,"end":382}]},{"name":"f503","type":13,"text":"Simplicity: No scripting language.","markups":[]},{"name":"24a7","type":1,"text":"Simplicity is the best heuristic to building a secure system. So the only feature of Nimiq is fast and secure payments from UserA to UserB. Nimiq intentionally doesn’t have a scripting language, because Ethereum already solves the smart contract problem better then we could ever do. We do not try to compete in this field. We want to be compatible with Ethereum such that Nimiq users can easily use Ethereum’s smart contract features if they want to.","markups":[]},{"name":"bf23","type":1,"text":"There is one exception to the “no smart contracts” approach: For the Lightning Network there will be a hashed timelock contract hardcoded into the protocol.","markups":[]},{"name":"e831","type":13,"text":"Blockchain Parameters (preliminary)","markups":[]},{"name":"4a6a","type":9,"text":"Block time: 1 minute (inspired by the results of On the Security and Performance of Proof of Work Blockchains)","markups":[{"type":3,"start":49,"end":109,"href":"https://eprint.iacr.org/2016/555.pdf","title":"","rel":"","anchorType":0},{"type":2,"start":49,"end":109}]},{"name":"05b9","type":9,"text":"Block reward: starts with 5 Nimiq (NIM); halves every ~2'100'000 blocks. This approach models Bitcoin’s approach, while taking into account that Nimiq’s block time is 10 times faster. (In the betanet the reward is just constant)","markups":[{"type":3,"start":94,"end":112,"href":"https://en.bitcoin.it/wiki/Controlled_supply","title":"","rel":"","anchorType":0}]},{"name":"5b0d","type":9,"text":"Max Block size: 1 MB","markups":[]},{"name":"318c","type":9,"text":"Difficulty adjustment: Every 10 blocks","markups":[]},{"name":"0d09","type":9,"text":"Total supply: 21 Mio Coins divisible by 10⁸ (just like Bitcoin)","markups":[]},{"name":"717b","type":13,"text":"Cross-Chain Compatibility","markups":[]},{"name":"5b93","type":1,"text":"Hash Timelock Contracts not just allow off-chain transactions and scalability. We will use them for cross-chain transactions to become compatible with the great work of projects like Ethereum and Bitcoin. This allows Nimiq users to use the advanced smart contract features of Ethereum and it allows users of other cryptocurrencies to easily exchange into Nimiq without an intermediary.","markups":[{"type":2,"start":0,"end":23}]},{"name":"4e97","type":3,"text":"Conclusion","markups":[]},{"name":"7ff3","type":1,"text":"Nimiq introduced Blockchain technology native to the World Wide Web. In comparison to conventional cryptocurrencies, this approach lowers barriers of entry by orders of magnitudes and will foster mass-adoption of cryptocurrencies.","markups":[]},{"name":"31ee","type":3,"text":"Stay Tuned","markups":[]},{"name":"c5db","type":1,"text":"This document is work-in-progress and will be updated regularly.","markups":[]},{"name":"2818","type":1,"text":"To stay tuned:","markups":[]},{"name":"d97e","type":9,"text":"Follow our Github repository","markups":[{"type":3,"start":0,"end":28,"href":"https://github.com/nimiq-network/core","title":"","rel":"","anchorType":0}]},{"name":"40b3","type":9,"text":"Join our Slack Channel","markups":[{"type":3,"start":0,"end":22,"href":"https://nimiq-slackin.herokuapp.com/","title":"","rel":"","anchorType":0}]},{"name":"c29a","type":9,"text":"Join our Telegram Channel","markups":[{"type":3,"start":0,"end":25,"href":"https://t.me/joinchat/AAAAAEJW-ozFwo7Er9jpHw","title":"","rel":"","anchorType":0}]},{"name":"39aa","type":9,"text":"Follow us on Twitter","markups":[{"type":3,"start":0,"end":20,"href":"https://twitter.com/nimiqnetwork","title":"","rel":"","anchorType":0}]},{"name":"38e5","type":9,"text":"Follow us on Youtube","markups":[{"type":3,"start":0,"end":20,"href":"https://www.youtube.com/channel/UCJ1qu3KXwfmkGwBkU8F1jQg","title":"","rel":"","anchorType":0}]},{"name":"2e00","type":13,"text":"Read More about Nimiq","markups":[]},{"name":"b638","type":9,"text":"Website","markups":[{"type":3,"start":0,"end":7,"href":"https://nimiq.com","title":"","rel":"nofollow noopener","anchorType":0}]},{"name":"672d","type":9,"text":"Beta Testnet","markups":[{"type":3,"start":0,"end":12,"href":"https://nimiq.com/betanet","title":"","rel":"nofollow noopener","anchorType":0}]},{"name":"6f3a","type":9,"text":"Whitepaper","markups":[{"type":3,"start":0,"end":10,"href":"https://medium.com/nimiq-network/nimiq-a-peer-to-peer-payment-protocol-native-to-the-web-ffd324bb084","title":"","rel":"","anchorType":0}]},{"name":"d48b","type":9,"text":"Team","markups":[{"type":3,"start":0,"end":4,"href":"https://nimiq.com/team","title":"","rel":"nofollow noopener","anchorType":0}]},{"name":"cca5","type":9,"text":"Contribution Terms","markups":[{"type":3,"start":0,"end":18,"href":"https://medium.com/nimiq-network/nimiq-network-token-sale-terms-9af2e7fd6228","title":"","rel":"","anchorType":0}]},{"name":"ceea","type":6,"text":"DISCLAIMER: None of the statements must be viewed as an endorsement or recommendation for Nimiq, any cryptocurrency, or investment product. Neither the information, nor any opinion contained herein constitutes a solicitation or offer by the creators or participants to buy or sell any securities or other financial instruments or provide any investment advice or service.","markups":[]}],"sections":[{"name":"ebdd","startIndex":0},{"name":"f09f","startIndex":86}]},"postDisplay":{"coverless":true},"metaDescription":"Nimiq is a frictionless peer-to-peer payment protocol for the World Wide Web. It is a next-generation Blockchain protocol combining elements of Bitcoin and Ethereum, streamlined for the web platform."},"virtuals":{"statusForCollection":"APPROVED","allowNotes":true,"previewImage":{"imageId":"1*7DSgRazsxGLPKNq0J0wmLw.png","filter":"","backgroundSize":"","originalWidth":1831,"originalHeight":651,"strategy":"resample","height":0,"width":0},"wordCount":1788,"imageCo