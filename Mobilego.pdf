.pie {
    margin: 45px auto;
    width: 150px;
    height: 150px;
    border-radius: 50%;
    background: #51b4c7;
    background-image: linear-gradient(to right, transparent 50%, #ea3f33 0);
}
.pie::before {
  content: '';
  display: block;
  margin-left: 50%;
  height: 100%;
  border-radius: 0 100% 100% 0 / 50%;
  background-color: inherit;
  transform-origin: left;
  transform: rotate(.025turn);
}

